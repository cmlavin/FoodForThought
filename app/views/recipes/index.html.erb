<div class="row">

<div class="col s10 push-s1 m8 push-m2 l6 push-l3">
<h1>All recipes:</h1>
<% @recipes.each do |recipe| %>
  <p> <%= link_to recipe.name, recipe_path(recipe.id)%></p>
<% end %>
</div></div>

<% if @recipes.blank? %>
  <h4>There are no recipes containing the term <%= params[:search] %>.</h4>
<% end %>
