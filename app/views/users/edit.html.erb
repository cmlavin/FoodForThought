<div class="row">

<div class="col s10 push-s1 m8 push-m2 l6 push-l3">
<h1>Edit User</h1>
<%= form_for @user do |f| %>
  <%= f.label :user_name %>
  <%= f.text_field :user_name %><br>

  <%= f.label :password %>
  <%= f.text_field :password %><br>

  <%= f.label :first_name %>
  <%= f.text_field :first_name %><br>

  <%= f.label :last_name %>
  <%= f.text_field :last_name %><br>

  <%= f.submit %>
<% end %>
</div></div>
